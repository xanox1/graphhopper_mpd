// Simplified custom model for moped routing in Netherlands
// This uses basic car access and adjusts speeds for moped regulations
//
// To use this custom model you need to add it to your profile:
// profiles:
//    - name: moped_nl
//      weighting: custom
//      custom_model_files: [moped_nl_model.json]

{
  "distance_influence": 90,
  "priority": [
    { "if": "!car_access", "multiply_by": "0" },
    { "if": "road_access == DESTINATION || road_access == PRIVATE", "multiply_by": "0.1" }
  ],
  "speed": [
    { "if": "true", "limit_to": "car_average_speed" },
    { "if": "true", "limit_to": "45" },
    { "if": "road_class == RESIDENTIAL", "limit_to": "30" },
    { "if": "road_class == LIVING_STREET", "limit_to": "15" }
  ]
}