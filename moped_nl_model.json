// This custom model creates a moped vehicle profile optimized for Netherlands routing.
// It includes Netherlands-specific speed limits and access rules for moped vehicles.
// Mopeds in Netherlands have specific regulations regarding cycleway access and speed limits.
//
// To use this custom model you need to set the following in config.yml:
// graph.encoded_values: moped_access, car_average_speed, road_access, road_class
// profiles:
//    - name: moped_nl
//      weighting: custom
//      custom_model_files: [moped_nl_model.json]

{
  "distance_influence": 90,
  "priority": [
    { "if": "moped_access == NO", "multiply_by": "0" },
    { "if": "road_access == DESTINATION || road_access == PRIVATE", "multiply_by": "0.1" },
    { "if": "moped_access == YES || moped_access == DESIGNATED", "multiply_by": "1.2" },
    { "if": "road_class == CYCLEWAY && moped_access == YES", "multiply_by": "1.0" },
    { "if": "road_class == TRACK", "multiply_by": "0.3" }
  ],
  "speed": [
    { "if": "true", "limit_to": "car_average_speed" },
    { "if": "true", "limit_to": "45" },
    { "if": "road_class == RESIDENTIAL", "limit_to": "30" },
    { "if": "road_class == LIVING_STREET", "limit_to": "15" },
    { "if": "road_class == CYCLEWAY", "limit_to": "25" }
  ]
}